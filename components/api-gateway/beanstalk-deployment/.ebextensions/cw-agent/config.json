{
  "agent": {
    "metrics_collection_interval": 10
  },
  "metrics": {
    "namespace": "Fizz/Gateway",
    "metrics_collected": {
      "mem": {
        "measurement": [
          "mem_used",
          "mem_cached",
          "mem_total",
          "mem_free"
        ],
        "metrics_collection_interval": 1
      }
    },
    "append_dimensions": {
      "InstanceId": "${aws:InstanceId}"
    },
    "force_flush_interval" : 30
  }
}